@model AccountingFinance

@{
    ViewData["Title"] = "Edit page";
}

<form asp-controller="Home" asp-action="Edit" method="post"> 
    <input hidden asp-for="Id" />
   <div class="container">
   <div class="form-group row">
            <label asp-for="CreatedAt" class="col-sm-2 col-form-label">Date</label>
      <div class="col-sm-10">
                <input type="date" asp-for="CreatedAt" />
       </div>
   </div>
    <div class="form-group row">
        <label asp-for="OfficeRentprice" class="col-sm-2 col-form-label">OfficeRentprice</label>
       <div class="col-sm-10">
        <input asp-for="OfficeRentprice" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="EmployeeSalary" class="col-sm-2 col-form-label">EmployeeSalary</label>
       <div class="col-sm-10">
        <input asp-for="EmployeeSalary" />
        </div>
    </div>
    <div class ="form-group row">
        <label asp-for="TaxPrice" class="col-sm-2 col-form-label">TaxPrice</label>
       <div class="col-sm-10">
        <input asp-for="TaxPrice" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="OfficeSupplyPrice" class="col-sm-2 col-form-label">OfficeSupply</label>
        <div class="col-sm-10">
			<input asp-for="OfficeSupplyPrice" />
		</div>
    </div>
    <div class="form-group row">
        <label asp-for="SoldSWPrice" class="col-sm-2 colcol-form-label">SoldSWPrice</label>
      <div class="col-sm-10">
        <input asp-for="SoldSWPrice" />
        </div>
    </div>
    <div>
        <button type="submit" class="btn btn-primary" >Update</button>
            <a asp-controller="home" asp-action="index" class="btn btn-danger">Cancel </a>
    </div>
    </div>
</form >